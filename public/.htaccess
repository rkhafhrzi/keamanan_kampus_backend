RewriteEngine On

# Pastikan semua request masuk ke index.php
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [QSA,L]
<FilesMatch "^\.env">
    Require all denied
</FilesMatch>

<FilesMatch "^composer\.(json|lock)$">
    Require all denied
</FilesMatch>
